    <script type="text/javascript">
      $(document).ready(function () {

        if (getAutoCookie("auto_i") != "") {
          var id = getAutoCookie("auto_i");
          var passwd = getAutoCookie(id);
        }
        if (getAutoCookie("auto_p") != "") {
          var phone = getAutoCookie("auto_p");
          var passwd = getAutoCookie(phone);
        }

        if (getAutoCookie("auto_i") != "") {
                      ajax_login(id, passwd);
                  } else if (getAutoCookie("auto_p") != "") {
                      ajax_login(phone, passwd);
                  } else { }


        // 검색조회
        if (document.getElementById("search_key_id")) {
          try {
            $("#search_key_id").autocomplete({
            source : function( request, response ) { 
              var term = $.trim(request.term);
              $.ajax({
                  url:'/lib/search/find_engine.php',
                  type:'POST',
                  dataType: 'json',
                  data: { stx : term,  mode: "at" },
                  success: function(data) {
                    response(
                      $.map(data, function(item) {
                        return {
                          label: item
                        }
                      })
                    );
                    $('#recommend_contents').css('display','none');
                    $('#ui-id-1').css('z-index',999);
                  },
                  error: function (jqXHR, textStatus, errorThrown) {
                    response(
                      $.map([], function(item) {
                        return {
                          label: item
                        }
                      })
                    );
                  }
              });         
            },
            focus: function(event, ui) {
              return false;
            },
            select : function(event, ui) {
              document.getElementById('search_key_id').value = ui.item.value;
              document.getElementById('main-search-btn').click();
            }
          });
          } catch (e) {
            // console.error(e);
          }
        }
      });


      //20180109 자동로그인 변경[로그인 이후 쿠키 저장] ;loing.php
      function ajax_login(key, value) {
        ajaxReturn_de(key, value).done(function (data) {

          if (getAutoCookie("auto_i") != "") {
            $.ajax({
              url: '/coopmall/login.php',
              type: 'post',
              data: { id: data.key, passwd: data.value, auto_log: "on", how_login: "id", darkhorse: "1", coop: "1", LgC: "1719889252", PJW: "1719889252", RN: "auto_login_cookie" },
              success: function (data) {
                                  $("#auto_login").html(data);
                              },
              error: function () {
              }
            });
          } else if (getAutoCookie("auto_p") != "") {
            $.ajax({
              url: '/coopmall/login.php',
              type: 'post',
              data: { phone: data.key, passwd: data.value, auto_log: "on", how_login: "phone", darkhorse: "1", coop: "1", LgC: "1719889252", PJW: "1719889252", RN: "auto_login_cookie" },
              success: function (data) {
                                  $("#auto_login").html(data);
                              },
              error: function () {
              }
            });
          } else { }

        });
      }

