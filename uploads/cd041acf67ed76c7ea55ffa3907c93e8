<script language="javascript">
function captureReturnKey(e) {
	if(e.keyCode==13 && e.srcElement.type != 'textarea')
	return false;
}
function detail_roll_img(str){
	if(str == "A"){
		for(i=1; i < 5; i++){
			if(com_plus != "on"){
				var plus = i;
			}else{
				var plus = plus + 1;
			}

			var goods_chk = document.getElementById('goods'+plus).style.display;

				if(goods_chk == "block"){
					document.getElementById("goods"+i).style.display = "none";
					plus = plus + 1;
					var com_plus = "on";
					document.getElementById("goods"+plus).style.display = "block";
				}else{
					if(document.getElementById('goods4').style.display == "block"){
						document.getElementById("goods4").style.display = "none";
						document.getElementById("goods1").style.display = "block";
					}else{
						document.getElementById("goods"+plus).style.display = "none";
					}
				}
		}
	}

	if(str == "B"){
		for(i = 4; i>0; i--){
			if(com_plus != "on"){
				var plus = i;
			}else{
				var plus = plus - 1;
			}

			var goods_chk = document.getElementById('goods'+plus).style.display;

			if(goods_chk == "block"){
				document.getElementById("goods"+i).style.display = "none";
				plus = plus - 1;
				var com_plus = "on";
				document.getElementById("goods"+plus).style.display = "block";
			}else{
				if(document.getElementById('goods1').style.display == "block"){
					document.getElementById("goods1").style.display = "none";
					document.getElementById("goods4").style.display = "block";
				}else{
					document.getElementById("goods"+plus).style.display = "none";
				}
			}
		}
	}
}

function frmdetail_submit() {
    var form = document.frmdetail;
    var good_codes = form.good_codes.value;
    var good_codes_arr =good_codes.split(";");
    good_codes = good_codes_arr[0];

    var chk_count = 0;
    var ashstr ='주문하실 상품은 공정무역 상품입니다.\n\n';

    var fair_ab ='';
    var fair_price ='';
    var fair_amount ='';

    if(document.getElementById(good_codes)!=null){

        fair_ab = document.getElementById(good_codes+'_fair').value;
        if(fair_ab =='B' || fair_ab =='D'){
            chk_count = chk_count +1;
            fair_price = document.getElementById(good_codes+'_fair_price').value;
			if(fair_ab =='B'){
				ashstr += '해당 상품에는 공정무역기금이 수량 당 ('+fair_price+'원)씩 합쳐집니다.';
			}else{
				ashstr += '해당 상품에는 국내친환경과일기금이 수량 당 ('+fair_price+'원)씩 합쳐집니다.';
			}
            ashstr += '\n';
        }
    }

    if(chk_count > 0){
        ashstr += '\n공정무역기금을 상품대금과 함께 납부하는 것에 동의하십니까?'

        if(confirm(ashstr) == false) {
			alert('주문을 취소하셨습니다.');
			return false;
        }
    }

	if(document.frmdetail.btnclick.value == "1") {
		alert("잠시 기다려 주세요. 현재 장바구니에 담고 있습니다.    ");
		return false;
	} else {
		document.frmdetail.btnclick.value = "1";
		return true;
	}
}

function big_view_goodimg(filename,dir) {
	var filename;
	var dir;
	var url  = 'big_view_goodimg.php?filename='+filename+'&owner='+dir;
	var name = 'viwephoto';
	var opt  = 'width=410, height=410, menubars=no, statusbar=no, scrollbars=yes, top=0, left=0';

	openview = window.open(url, name, opt);
}

function MM_swapImgRestore() { //v3.0
	var i,x,a=document.MM_sr; for(i=0;a&&i<a.length&&(x=a[i])&&x.oSrc;i++) x.src=x.oSrc;
}

function MM_preloadImages() { //v3.0
	var d=document; if(d.images){ if(!d.MM_p) d.MM_p=new Array();
		var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
		if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}

function MM_findObj(n, d) { //v4.01
	var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
		d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
	if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
	for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
	if(!x && d.getElementById) x=d.getElementById(n); return x;
}

function MM_swapImage() { //v3.0
	var i,j=0,x,a=MM_swapImage.arguments; document.MM_sr=new Array; for(i=0;i<(a.length-2);i+=3)
		if ((x=MM_findObj(a[i]))!=null){document.MM_sr[j++]=x; if(!x.oSrc) x.oSrc=x.src; x.src=a[i+2];}
}

function ReadCookie(name) {
	var label = name + "=" ;
	var labelLen = label.length ;
	var cLen = document.cookie.length;
	var i = 0;
	while (i < cLen){
		var j = i + labelLen;
		if (document.cookie.substring(i,j) == label) {
			var cEnd = document.cookie.indexOf(";",j);
			if (cEnd == -1){
				cEnd = document.cookie.length;
			}
			return unescape(document.cookie.substring(j,cEnd));
		}
		i++;
	}
	return "";
}

function setCookie( name, value, expirehours ) {
	var todayDate = new Date();
	todayDate.setHours( todayDate.getHours() + expirehours );
	document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
}

function setCookie_quick_menu(value){
	var expire = new Date();
	if (document.cookie.length > 0){	// 쿠키 생성여부
		var search = "recent_goods=";
		var offset_cookie = document.cookie.indexOf(search)
		if (offset_cookie != -1){	// 최근내역 쿠키 생성여부
			offset_cookie += search.length;
			var end = document.cookie.indexOf(";", offset_cookie);
			if (end == -1) end = document.cookie.length;
			var tmp_value = document.cookie.substring(offset_cookie, end);	// 현재 저장되어있는 쿠키값

			// 갯수 카운트 필요함.  10개 미만.. 10개 이상.
			var goods_cookie_arr = tmp_value.split("@");
			var cookie_count = goods_cookie_arr.length;

			if(tmp_value.length > 0){
				var offset_cookie2 = tmp_value.indexOf(value);
				if (offset_cookie2 == -1){	 // 없을경우에만 추가
					if(cookie_count > 9){	// 쿠키가 9개 이상 발생하게 된다면..
						var first_cookie = goods_cookie_arr[0];
						first_cookie = first_cookie+'@';
						tmp_value = tmp_value.replace(first_cookie, '');

						value = tmp_value+'@'+ value;	//기본 쿠키값 개선
						expire.setDate(expire.getDate() + 2);	// 만료기간 1일 설정
						cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
						document.cookie = cookies;

					}else{
						value = tmp_value+'@'+ value;	//기본 쿠키값 개선
						expire.setDate(expire.getDate() + 2);	// 만료기간 1일 설정
						cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
						document.cookie = cookies;
					}
				}else{
					var chg_cookie = value;
					var first_cookie = goods_cookie_arr[0];

					if(first_cookie == chg_cookie){	 // 제일 처음봤던 물품의 경우 업데이트 해야함.
						if(cookie_count > 1){	// 쿠키가 1개 이상일 경우
							var first_cookie = goods_cookie_arr[0];
							first_cookie = first_cookie+'@';
							tmp_value = tmp_value.replace(first_cookie, '');

							value = tmp_value+'@'+ value;	//기본 쿠키값 개선
							expire.setDate(expire.getDate() + 2);
							cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
							document.cookie = cookies;
						}
					}else{
						chg_cookie = value+'@';
						var offset_cookie3 = tmp_value.indexOf(chg_cookie);
						if (offset_cookie3 == -1){	 // 최근내역
							return false;
						}else{
							chg_cookie = '@'+value;
							tmp_value = tmp_value.replace(chg_cookie, '');
							value = tmp_value+'@'+ value;
							expire.setDate(expire.getDate() + 2);
							cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
							document.cookie = cookies;
						}
					}
				}
			}
		}else{
			expire.setDate(expire.getDate() + 2);	// 만료기간 2일 설정
			cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
			document.cookie = cookies;
		}
	}else{
		expire.setDate(expire.getDate() + 2);	// 만료기간 2일 설정
		cookies = 'recent_goods=' + value + '; path=/; expires='+expire.toGMTString() + ';';
		document.cookie = cookies;
	}
}

