function go_cart_subscribe() {

	var form = document.frmdetail;
	var good_codes = form.good_codes.value;
    var good_codes_arr =good_codes.split(";");
    good_codes = good_codes_arr[0];

	// break; 
	var newForm = $('<form></form>'); 
	newForm.attr("name","newForm");
	newForm.attr("method","post");
	newForm.attr("action","/coopmall/goods_add.php?act=detail&cart_type=subscribe&apptype=&gg=M08&key=");
	// create element & set attribute (input) 
	newForm.attr("target","");
	// append form (to body) 
	newForm.append($('<input/>', {type: 'hidden', name: 'good_codes', value: '10003BS300;' }));
	newForm.append($('<input/>', {type: 'hidden', name: 'cart_type', value:  'subscribe' }));

	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_code', value:  '10003BS300' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_name', value:  '글라스밀폐용기' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_a_cost', value:  '9400' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_b_cost', value:  '9400' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_flock', value:  'M08' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300', value: $('#box_10003BS300').val() }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_fair', value:  'A' }));
	newForm.append($('<input/>', {type: 'hidden', name: '10003BS300_fair_price', value:  '' }));

	newForm.appendTo('#subscribe_goods_add');
	newForm.submit();

}

